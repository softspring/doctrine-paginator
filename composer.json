{
    "name": "softspring/doctrine-paginator",
    "description": "This library provides a paginator for doctrine query builder",
    "type": "library",
    "license": "MIT",
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": ">=7.4",
        "doctrine/orm": "^2.11",
        "softspring/doctrine-query-filters": "^5.0",
        "symfony/http-foundation": "^5.4|^6.0",
        "symfony/form": "^5.4|^6.0",
        "symfony/validator": "^5.4|^6.0"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "9999999-dev",
        "phpunit/phpunit": "^9.5"
    },
    "autoload": {
        "psr-4": {
            "Softspring\\Component\\DoctrinePaginator\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Softspring\\Component\\DoctrinePaginator\\Tests\\": "tests/"
        }
    }
}
