{
    "name": "softspring/doctrine-paginator",
    "description": "This library provides a paginator for doctrine query builder",
    "type": "library",
    "license": "MIT",
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": ">=8.1",
        "doctrine/orm": "^2.11|^2.17@dev",
        "doctrine/collections": "^2.0 | ^3.0",
        "softspring/doctrine-query-filters": "^5.2",
        "symfony/http-foundation": "^5.4|^6.0|^7.0",
        "symfony/form": "^5.4|^6.0|^7.0",
        "symfony/validator": "^5.4|^6.0|^7.0",
        "symfony/routing": "^5.4|^6.0|^7.0"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^3.40",
        "phpstan/phpstan": "^1.10",
        "phpunit/phpunit": "^9.5 || ^10.0"
    },
    "autoload": {
        "psr-4": {
            "Softspring\\Component\\DoctrinePaginator\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Softspring\\Component\\DoctrinePaginator\\Tests\\": "tests/"
        }
    },
    "config": {
        "sort-packages": true
    }
}
