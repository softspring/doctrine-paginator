{
    "name": "softspring/doctrine-paginator",
    "description": "This library provides a paginator for doctrine query builder",
    "type": "library",
    "license": "MIT",
    "minimum-stability": "dev",
    "prefer-stable": false,
    "require": {
        "php": ">=8.0",
        "doctrine/orm": "^2.11|^2.17@dev",
        "doctrine/collections": "^1.8 | ^2.0",
        "softspring/doctrine-query-filters": "^5.1",
        "symfony/http-foundation": "^5.4|^6.0|^7.0@dev",
        "symfony/form": "^5.4|^6.0|^7.0@dev",
        "symfony/validator": "^5.4|^6.0|^7.0@dev",
        "symfony/routing": "^5.4|^6.0|^7.0@dev"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^3.14",
        "phpstan/phpstan": "^1.9",
        "phpunit/phpunit": "^9.5"
    },
    "autoload": {
        "psr-4": {
            "Softspring\\Component\\DoctrinePaginator\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Softspring\\Component\\DoctrinePaginator\\Tests\\": "tests/"
        }
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "branch-alias": {
            "dev-5.0": "5.0.x-dev",
            "dev-5.1": "5.1.x-dev",
            "dev-5.2": "5.2.x-dev"
        }
    }
}
